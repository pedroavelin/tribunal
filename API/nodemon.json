{
  "restartable": "rs",
  "ignore": [".git", "node_modules/"],
  "verbose": false,
  "execMap": {
    "js": "node"
  },
  "events": {
    "restart": "echo \"App restarted\""
  },
  "watch": ["server.js", "app.js", "routes/", "controllers/"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}